{
    "version": 2,
    "functions": {
        "server/index.js": {
            "runtime": "vercel-node@latest"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}