{
    "version": 2,
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/index.js"
        },
        {
            "src": "/chat.css",
            "dest": "/public/chat.css"
        },
        {
            "src": "/chat.js",
            "dest": "/public/chat.js"
        },
        {
            "src": "/app.js",
            "dest": "/public/app.js"
        },
        {
            "src": "/styles.css",
            "dest": "/public/styles.css"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}